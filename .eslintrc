{
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "max-len": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "prefer-const": 0,
    "no-param-reassign": [2, { "props": false }]
  },
  "globals": {
    "document": true,
    "window": true,
    "Image": true,
    "FileReader": true,
    "fetch": true,
    "navigator": true
  }
}
